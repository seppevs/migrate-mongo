    "mongodb": "^4.4.1 || ^5.0.0 || ^6.0.0  || ^7.0.0"
  },
  "devDependencies": {
    "chai": "^6.2.1",
    "coveralls": "^3.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-mocha": "^11.2.0",
    "mocha": "^11.7.5",
    "nyc": "^17.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^21.0.0"
